{
    "name": "artem328/laravel-yandex-kassa",
    "type": "library",
    "description": "Yandex Kassa integration with Laravel framework",
    "keywords": [
        "laravel", "yandex", "kassa", "money",
        "яндекс", "касса", "деньги"
    ],
    "homepage": "https://github.com/artem328/laravel-yandex-kassa",
    "license": "MIT",
    "authors": [
        {"name": "Artem Vozhzhov", "email": "vojjov.artem@ya.ru", "role": "Developer"}
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "autoload": {
        "psr-4": {
            "Artem328\\LaravelYandexKassa\\": "src/LaravelYandexKassa"
        },
        "files": [
            "src/LaravelYandexKassa/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artem328\\LaravelYandexKassa\\YandexKassaServiceProvider"
            ],
            "aliases": {
                "YandexKassa": "Artem328\\LaravelYandexKassa\\Facades\\YandexKassa"
            }
        }
    }
}
